{
  "name": "@rottitime/create-starter-kit",
  "version": "1.0.5",
  "bin": "./bin/cli.js",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/rottitime/frontend-starterkit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rottitime/frontend-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/rottitime/frontend-starterkit/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:watch": "DEBUG_PRINT_LIMIT=1000000 jest --watch",
    "test:all": "next lint && npm run test:tsc && jest && npm run test:e2e",
    "test:tsc": "tsc --noemit",
    "test:coverage": "jest --coverage",
    "test:e2e": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e --record --key b099ca38-0f20-4db5-a73d-ebd8ca9298e9\"",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:all",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "postinstall": "xstate typegen \"./src/**/*.ts?(x)\"",
    "cypress:open": "cypress open",
    "cypress:headless": "cypress run --headless"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "@tanstack/react-query": "^4.22.0",
    "@tanstack/react-query-devtools": "^4.22.0",
